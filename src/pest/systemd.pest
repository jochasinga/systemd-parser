WHITESPACE = _{ " " }
COMMENT = _{ ("#" | ";") ~ (!NEWLINE ~ ANY)* ~ NEWLINE }
char = { ASCII_ALPHANUMERIC | WHITESPACE | "." | "_" | "/" | "-"  }
name = @{ char+ }
value = { (char | "=" | ":" )* }
section = { "[" ~ name ~ "]" }
property = { name ~ "=" ~ value }

file = {
    SOI ~
    ((section | property)? ~ NEWLINE)* ~
    EOI
}

